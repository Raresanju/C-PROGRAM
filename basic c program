#include <stdio.h>
int main() {
    int height,weight;
    printf("Enter your height:");
    printf("\nEnter your weight ");
    scanf("%d %d",&height,&weight);
    if(height>=158 && weight>50){
        printf("Congrats!! you are eligible");
        }
    else
    {
        printf("Sorry you are not eligible");
    }
    return 0;
}
